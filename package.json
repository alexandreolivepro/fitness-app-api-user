{
  "name": "fit-users-api",
  "version": "0.1.0",
  "scripts": {
    "start": "npm run serve",
    "build": "npm run build-ts",
    "build:ci": "npm run build-ts",
    "serve": "node dist/main.js",
    "serve-debug": "nodemon --inspect src/main.ts",
    "watch-node": "nodemon src/main.js",
    "build-ts": "tsc",
    "watch-ts": "tsc -w",
    "watch": "concurrently -k -p \"[{name}]\" -n \"TypeScript,Node\" -c \"cyan.bold,yellow.bold\" \"npm run watch-ts\" \"npm run watch-node\"",
    "test": "jest --forceExit --coverage --verbose --config ./jest.spec.config.js ",
    "e2e": "jest --forceExit --coverage --verbose --config ./jest.e2e.config.js ",
    "e2e:no": "npm run e2e -- --config ./src/config/e2e/jest.config.js",
    "lint": "eslint . --ext .ts,.js",
    "lint:fix": "npm run lint -- --fix"
  },
  "author": "Alexandre Olive",
  "license": "MIT",
  "dependencies": {
    "ajv": "^7.1.1",
    "async": "^3.2.0",
    "bcrypt": "^5.0.0",
    "bluebird": "^3.7.2",
    "body-parser": "^1.19.0",
    "compression": "^1.7.4",
    "connect-mongo": "^3.2.0",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-oauth-server": "^2.0.0",
    "express-swagger-generator": "^1.1.17",
    "express-validator": "^6.9.2",
    "method-override": "^3.0.0",
    "mocha": "^8.3.0",
    "moment": "^2.29.1",
    "mongodb": "^3.6.4",
    "mongoose": "^5.11.17",
    "mongoose-to-csv": "^0.1.1",
    "nodemailer": "^6.4.18",
    "oauth2-server": "^3.1.1",
    "object-path": "^0.11.5",
    "passport": "^0.4.1",
    "passport-http": "^0.3.0",
    "passport-local": "^1.0.0",
    "ts-node": "^9.1.1"
  },
  "devDependencies": {
    "@types/async": "^3.2.5",
    "@types/babel__core": "^7.1.12",
    "@types/bcrypt": "^3.0.0",
    "@types/bcrypt-nodejs": "^0.0.31",
    "@types/bluebird": "^3.5.33",
    "@types/body-parser": "^1.19.0",
    "@types/chai": "^4.2.15",
    "@types/compression": "^1.7.0",
    "@types/connect-mongo": "^3.1.3",
    "@types/dotenv": "^8.2.0",
    "@types/express": "^4.17.11",
    "@types/express-oauth-server": "^2.0.3",
    "@types/jest": "^26.0.20",
    "@types/method-override": "0.0.31",
    "@types/mongodb": "^3.6.8",
    "@types/node": "^14.14.31",
    "@types/object-path": "^0.11.0",
    "@types/passport": "^1.0.6",
    "@types/passport-http": "^0.3.8",
    "@types/passport-local": "^1.0.33",
    "@types/request": "^2.48.5",
    "@types/stack-utils": "^2.0.0",
    "@types/supertest": "^2.0.10",
    "@types/swagger-ui-express": "^4.1.2",
    "@types/winston": "^2.4.4",
    "@typescript-eslint/eslint-plugin": "^4.15.1",
    "@typescript-eslint/parser": "^4.15.1",
    "chai": "^4.3.0",
